{
  "name": "react-bootstrap-webpack-starter",
  "version": "0.3.2",
  "description": "react js + bootstrap + webpack starter",
  "main": "src/index.js",
  "scripts": {
    "dev": "NODE_ENV=dev webpack",
    "prod": "NODE_ENV=production webpack -p --config webpack.production.config.babel.js ",
    "start": "NODE_ENV=dev babel-node --presets es2015 ./server.hot.reload.js",
    "dev-win": "env NODE_ENV=dev webpack",
    "prod-win": "env NODE_ENV=production webpack -p --config webpack.production.config.babel.js ",
    "start-win": "env NODE_ENV=dev babel-node --presets es2015 ./server.hot.reload.js"
  },
  "keywords": [
    "react",
    "ES6",
    "ES2015",
    "bootstrap",
    "starter",
    "webpack",
    "hot-reload",
    "hot reload"
  ],
  "author": "Erwan DATIN (MacKentoch)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.6.0",
    "eslint-plugin-react": "^4.2.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "animate.css": "^3.5.1",
    "babel-polyfill": "^6.7.4",
    "barracks": "^8.0.2",
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.3",
    "font-awesome": "^4.5.0",
    "hash-match": "^1.0.2",
    "jquery": "^2.2.1",
    "react": "^15.2.1",
    "react-addons-shallow-compare": "^15.2.1",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.2.1",
    "react-tap-event-plugin": "^1.0.0",
    "sheet-router": "^3.1.1",
    "superagent": "^1.8.3",
    "whatwg-fetch": "^1.0.0"
  }
}
